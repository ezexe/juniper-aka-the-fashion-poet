                              <spsxs:example>866-245-8100</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="PrimaryFax" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Fax number of contact listed</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>612-435-9401</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
