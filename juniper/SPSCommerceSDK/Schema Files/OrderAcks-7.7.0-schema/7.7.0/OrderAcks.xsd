    ADVANCES DICTATE, OR WILL BE MERCHANTABLE OR SUITABLE FOR ANY PARTICULAR PURPOSE OR WORKABLE IN ANY
    APPLICATION, OR OTHERWISE. THE USE OF THE SCHEMA AND RESULTANT MESSAGE BY ANY PARTY ARE WITH THE EXPRESS
    UNDERSTANDING THAT SPS COMMERCE ASSUMES NO LIABILITY FOR ANY CLAIM, DAMAGE OR LOSS OF ANY KIND OR NATURE.:
    
    COMMENTS:
    
    The mandatory XML elements, specific codes and qualifiers used between Trading partners are defined in
    separate trading partner guidelines between the partners and are not part of this schema and resultant
    XML message.-->
<xs:schema xmlns:spsxs="http://www.spscommerce.com/RSX/SPSSchemaDefinition" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.spscommerce.com/RSX" targetNamespace="http://www.spscommerce.com/RSX" elementFormDefault="qualified" attributeFormDefault="unqualified" version="7.7.0">
  <xs:element name="OrderAcks">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="OrderAck" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="OrderAck" type="OrderAckType"/>
  <xs:complexType name="OrderAckType">
    <xs:sequence>
      <xs:element name="Meta" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Contains internal SPS information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="SenderUniqueID" type="xs:string" minOccurs="0"/>
            <xs:element name="SenderCompanyName" type="xs:string" minOccurs="0"/>
            <xs:element name="ReceiverUniqueID" type="xs:string" minOccurs="0"/>
            <xs:element name="ReceiverCompanyName" type="xs:string" minOccurs="0"/>
            <xs:element name="IsDropShip" type="xs:boolean" minOccurs="0"/>
            <xs:element name="InterchangeControlNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupControlIdentifier" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupControlNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="DocumentControlIdentifier" type="xs:string" minOccurs="0"/>
            <xs:element name="DocumentControlNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="InterchangeSenderID" type="xs:string" minOccurs="0"/>
            <xs:element name="InterchangeReceiverID" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupSenderID" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupReceiverID" type="xs:string" minOccurs="0"/>
            <xs:element name="BatchPart" type="xs:string" minOccurs="0"/>
            <xs:element name="BatchTotal" type="xs:int" minOccurs="0"/>
            <xs:element name="BatchID" type="xs:string" minOccurs="0"/>
            <xs:element name="Comments" type="xs:string" minOccurs="0"/>
            <xs:element name="Validation" type="xs:string" minOccurs="0"/>
            <xs:element name="OrderManagement" type="xs:string" minOccurs="0"/>
            <xs:element name="Version" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>XML Version number</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Header">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="OrderHeader">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="TradingPartnerId">
                    <xs:annotation>
                      <xs:documentation>Unique internal identifier defined by SPS Commerce which identifies the relationship</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>000ALLTESTID</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="15"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="PurchaseOrderNumber" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Identifying number for the purchase order assigned by the buying organization</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>1010101010101</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TsetPurposeCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying purpose or function of the transmission</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PrimaryPOTypeCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code indicating the specific details regarding the ordering document</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PrimaryPOTypeDescription" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free form text to describe the specific details regarding the ordering document</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>This is a test. Do not ship</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AdditionalPOTypeCodes" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>This group should be used if the associated normalized fields already contain information.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="POTypeCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code indicating additional details regarding the ordering document</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="POTypeDescription" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Free form text to describe the additional details regarding the ordering document</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>This is a test. Do not ship</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="PurchaseOrderDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Date the purchase order was created</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-08-29</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PurchaseOrderTime" type="xs:time" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Time the purchase order was created</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>21:05:49-06:00</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ReleaseNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Identifying number for the purchase order relating back to the original blanket order</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>XYZ999999</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AcknowledgementNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Identifying number for the acknowledgement assigned by the acknowledger</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>977650397</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AcknowledgementType" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code defining the vendor's status of the order as well as how much detail is being provided in the acknowledgement</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ShipCompleteCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code to identify a specific requirement or agreement of sale. Should only be used to indicate if an item can be placed on backorder</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="InternalOrderNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>ERP generated number assigned as unique identifier for each incoming order</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>456789-123</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="InternalOrderDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>ERP generated internal date for each incoming order</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-09-18</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AcknowledgementDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>PO Acknowledgement Date</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-10-15</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AcknowledgementTime" type="xs:time" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>PO Acknowledgement Time/Original Requested Ship Time</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>15:34:56-05:00</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="BuyersCurrency" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Buyers Alpha ISO Currency Code[http://www.iso.org/iso/home/standards/currency_codes.htm]</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="SellersCurrency" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Sellers Alpha ISO Currency Code[http://www.iso.org/iso/home/standards/currency_codes.htm]</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ExchangeRate" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The rate to be applied to convert one currency to another</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>1.03157</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Department" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Name or number identifying an area wherein merchandise is categorized within a store</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>026</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DepartmentDescription" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free form text to describe the name or number identifying an area wherein merchandise is categorized within a store</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>Electronics</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Vendor" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Number assigned by buyer that uniquely identifies the vendor</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>99999999</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="JobNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Project number assigned to a standard reorder purchase order</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>777770000555555</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Division" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Different entities belonging to the same parent company</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>Sam's Club</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CustomerAccountNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>End consumer's account number</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>123456789</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CustomerOrderNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>End consumer's order number</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>987654321</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DocumentVersion" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Used to communicate the version number of the current document</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>4</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DocumentRevision" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Used to communicate the revision number of the current document</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="PaymentTerms" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="TermsType" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying type of payment terms</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsBasisDateCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying the beginning of the terms period</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsTimeRelationCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code relating the TermsBasisDateCode to a date, time, or period</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDiscountPercentage" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Terms discount percentage available to the purchaser if an invoice is paid on or before the TermsDiscountDate. Percentages should be represented as real numbers[0% through 100% should be normalized to 0.0 through 100.00]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>5.62</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDiscountDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Date by which payment or invoice must be received in order to receive the discount noted</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-11-28</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDiscountDueDays" type="xs:int" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Number of days by which payment or invoice must be received in order to receive the discount noted</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>60</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsNetDueDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Date by which total invoice amount is due[discount not applicable]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-11-15</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsNetDueDays" type="xs:int" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Number of days until total invoice amount is due[discount not applicable]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>90</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDiscountAmount" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Discount amount available to the purchaser if an invoice is paid on or before the TermsDiscountDate</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>1845.08</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDeferredDueDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Date by which the payment is due if terms have been deferred</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-10-25</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDeferredAmountDue" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The monetary amount which the payment is due if terms may have been deferred</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>215.28</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PercentOfInvoicePayable" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Percentages should be represented as real numbers[0% through 100% should be normalized to 0.0 through 100.00]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>12.5</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDescription" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>A free-form description to clarify the terms</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>Must be paid in full by end of year</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDueDay" type="xs:int" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Numeric value for day of month</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>28</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PaymentMethodCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Indication of the instrument of payment</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PaymentMethodID" type="xs:string" minOccurs="0">
                    <xs:annotation spsxs:longDescription="i.e. check number or letter of credit number">
                      <xs:documentation>Used to communicate the data of the PaymentMethodCode</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>999888777</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="LatePaymentChargePercent" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Indicates that this is the percentage applied to a base amount used to determine a late payment charge</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>15.2</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsStartDate" type="xs:date" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Date from which payment terms are calculated</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2013-09-30</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TermsDueDateQual" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying the method to be used for payment in conjunction with due date</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AmountSubjectToDiscount" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Monetary amount upon which the terms discount amount is calculated</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>68.18</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DiscountAmountDue" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Amount due if all discount terms are met</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>97.45</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="Dates" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="DateTimeQualifier" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code specifying the type of date</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Date" type="xs:date" minOccurs="0"/>
                  <xs:element name="Time" type="xs:time" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>All standard XML formats are accepted.</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>16:13:03-05:00</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DateTimePeriod" type="xs:string" minOccurs="0"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="Contacts" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="ContactTypeCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying a type of contact</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ContactName" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free-form name</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>SPS Commerce</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PrimaryPhone" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Phone number of contact listed</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>866-245-8100</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PrimaryFax" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Fax number of contact listed</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>612-435-9401</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PrimaryEmail" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>E-mail address for contact listed</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>info@spscommerce.com</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AdditionalContactDetails" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>This group should be used if the associated normalized fields already contain information.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="ContactQual" type="xs:string" minOccurs="0"/>
                        <xs:element name="ContactID" type="xs:string" minOccurs="0"/>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ContactReference" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Additional field to clarify a contact</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>ext: 451</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="Address" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="AddressTypeCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying an organizational entity, a physical location, property, or an individual</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="LocationCodeQualifier" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying the structure or format of the related location number[s]</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AddressLocationNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
