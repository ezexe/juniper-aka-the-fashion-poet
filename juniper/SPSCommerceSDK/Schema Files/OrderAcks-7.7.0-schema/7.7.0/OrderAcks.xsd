                  <xs:element name="AllowChrgRate" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Value expressed in monetary currency that applies to the given quantity [i.e. The rate is $5.00 off one dozen]. [AllowChrgRate, AllowChrgQty, AllowChrgQtyUOM fields should all be sent in conjunction with each other]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>2.43</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AllowChrgQtyUOM" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The unit of measure used in relation with AllowChrgQty[AllowChrgRate, AllowChrgQty, AllowChrgQtyUOM fields should all be sent in conjunction with each other]</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AllowChrgQty" type="xs:decimal" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Numeric value of quantity to which the allowance or charge applies[AllowChrgRate, AllowChrgQty, AllowChrgQtyUOM fields should all be sent in conjunction with each other]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>8</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AllowChrgHandlingCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code indicating method of handling for an allowance or charge</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ReferenceIdentification" type="xs:string" minOccurs="0"/>
                  <xs:element name="AllowChrgHandlingDescription" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free-form textual description of the allowance or charge</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>This will cover the cost of shipping</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="OptionNumber" type="xs:string" minOccurs="0"/>
                  <xs:element name="ExceptionNumber" type="xs:string" minOccurs="0"/>
                  <xs:element name="AllowChrgQty2" type="xs:decimal" minOccurs="0"/>
                  <xs:element name="LanguageCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code designating the language used in text. ISO 639 language code</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>ENG</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CalculationSequence" type="xs:int" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Used to indicate at what point a value should be used in calculations where orders of operations matter [i.e. 1 would indicate that the related value should be taken first].</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>1</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Taxes" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="TaxTypeCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Identification of the type of duty, tax, or fee applicable to commodities or of tax applicable to services</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="TaxAmount" type="xs:decimal" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>The monetary amount applied</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>1845.08</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="TaxPercentQual" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code indicating on what basis a tax percent is calculated</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="TaxPercent" type="xs:decimal" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>The percentage that is applied to determine the tax amount. Percentages should be represented as real numbers[0% through 100% should be normalized to 0.0 through 100.00]</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>8.50</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="JurisdictionQual" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code identifying the source of the data used in the Tax JurisdictionCode</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="JurisdictionCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code represents the City, State, or Providence[tax authority] associated with the Tax group</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>07034</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="TaxExemptCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code indicating whether the header or item level data is exempt[or not exempt] for the tax and taxing authority indicated in JurisdictionCode</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="RelationshipCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code identifying whether the tax amount is included in the total transaction amount</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="PercentDollarBasis" type="xs:decimal" minOccurs="0">
                          <xs:annotation spsxs:longDescription="i.e. Multiplying the TaxPercent by the PecentDollarBasis yields the TaxAmount">
                            <xs:documentation>Monetary amount that is used when calculating a tax, allowance, or charge amount.</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>2000</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="TaxHandlingCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code indicating method of handling for a tax</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="TaxID" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Number assigned to a purchaser[buyer, orderer] by a tax jurisdiction[state, country, etc]</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>99990000</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AssignedID" type="xs:string" minOccurs="0"/>
                        <xs:element name="Description" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Free-form textual description to clarify the related data elements and their content</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>New products only. Do not reuse packaging</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="FOBRelatedInstruction" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="FOBPayCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying payment terms for transportation charges</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="FOBLocationQualifier" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying the party or location responsible for organizing and paying for transportation</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="FOBLocationDescription" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free-form textual description of the location at which ownership of goods is transferred</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>Minnesota</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="FOBTitlePassageCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code describing the location of ownership of the goods changes</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="FOBTitlePassageLocation" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Location of ownership of the goods</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>Minneapolis MN</spsxs:example>
                        <spsxs:example>South Korea</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TransportationTermsType" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying the source of the transportation terms</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TransportationTerms" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code that defines to the recipient what types of fees are associated with the movement of goods. By default, if you do not send the TransportationTermsType, you are using the standard codes from the Incoterms website [http://www.iccwbo.org/products-and-services/trade-facilitation/]</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="RiskOfLossCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code specifying which party should bear the burden for risk of damage occurring to goods after the sale but before delivery</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Description" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free-form textual description to clarify the related data elements and their content</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>New products only. Do not reuse packaging</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="CarrierInformation" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="StatusCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code indicating the status of an order or shipment or the disposition of any difference between the quantity ordered and the quantity shipped for a line item or transaction</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CarrierTransMethodCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code specifying the method or type of transportation for the shipment</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CarrierAlphaCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Standard Carrier Alpha Code[SCAC] - 2 to 4 digit alphabetic code assigned by the National Motor Freight Traffic Association identifying transportation companies</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>EMSY</spsxs:example>
                        <spsxs:example>UPGF</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CarrierRouting" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free-form description of the routing/requested routing for shipment or the originating carrier's identity</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>ONTRAC</spsxs:example>
                        <spsxs:example>UPS Ground Freight</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="EquipmentDescriptionCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code identifying type of equipment used for shipment</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CarrierEquipmentInitial" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Prefix or alphabetic part of an equipment unit's identifying number</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>FEDX</spsxs:example>
                        <spsxs:example>XBGQ</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CarrierEquipmentNumber" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Sequencing or serial part of an equipment unit's identifying number[Pure numeric form for equipment number is preferred]</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>4806196209</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="EquipmentType" type="xs:string" minOccurs="0"/>
                  <xs:element name="OwnershipCode" type="xs:string" minOccurs="0"/>
                  <xs:element name="RoutingSequenceCode" type="xs:string" minOccurs="0"/>
                  <xs:element name="TransitDirectionCode" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The point of origin and point of direction</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="TransitTimeQual" type="xs:string" minOccurs="0"/>
                  <xs:element name="TransitTime" type="xs:decimal" minOccurs="0"/>
                  <xs:element name="ServiceLevelCodes" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="ServiceLevelCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code indicating the level of transportation service or the billing service offered by the transportation carrier</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="Address" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="AddressTypeCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code identifying an organizational entity, a physical location, property, or an individual</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="LocationCodeQualifier" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code identifying the structure or format of the related location number[s]</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AddressLocationNumber" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Unique value assigned to identify a location</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>11111</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AddressName" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Primary free-form textual description of a location</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>SPS Commerce</spsxs:example>
                              <spsxs:example>The White House</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AddressAlternateName" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Additional free-form description of a location</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>ATTN: Marketing</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AddressAlternateName2" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Additional free-form description of a location</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>ATTN: John Doe</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Address1" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Address information</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>1600 Pennsylvania Ave NW</spsxs:example>
                              <spsxs:example>333 South 7th Street</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Address2" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Address information</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>ATTN: The President</spsxs:example>
                              <spsxs:example>Suite 1000</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Address3" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Address information</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>333 South Seventh Street Tower</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Address4" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Address information</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>10th Floor</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="City" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Free-form text for city name</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>Minneapolis</spsxs:example>
                              <spsxs:example>Washington</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="State" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code[Standard State/Province] as defined by appropriate government agency</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>DC</spsxs:example>
                              <spsxs:example>MN</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="PostalCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>International postal zone excluding punctuation and blanks[Zip Code for United States]</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>20500</spsxs:example>
                              <spsxs:example>55402</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Country" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Human readable description identifying the country</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>US</spsxs:example>
                              <spsxs:example>USA</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="LocationID" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Provides further description to the address information. This can be either text or an ID</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>555555</spsxs:example>
                              <spsxs:example>East Location, next to interstate</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="CountrySubDivision" type="xs:string" minOccurs="0"/>
                        <xs:element name="AddressTaxIdNumber" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Unique number assigned by the relevant tax authority to identify a party for use in relation to Value Added Tax[VAT]</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>999999</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AddressTaxExemptNumber" type="xs:string" minOccurs="0"/>
                        <xs:element name="Dates" minOccurs="0" maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="DateTimeQualifier" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>Code specifying the type of date</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="Date" type="xs:date" minOccurs="0"/>
                              <xs:element name="Time" type="xs:time" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>All standard XML formats are accepted.</xs:documentation>
                                  <xs:appinfo>
                                    <spsxs:example>16:13:03-05:00</spsxs:example>
                                  </xs:appinfo>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="DateTimePeriod" type="xs:string" minOccurs="0"/>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="SealNumbers" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="SealStatusCode" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Code indicating condition of door seal upon arrival</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="SealNumber" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Stamped lock that is on the container</xs:documentation>
                            <xs:appinfo>
                              <spsxs:example>AA1111111</spsxs:example>
                            </xs:appinfo>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="RegulatoryCompliances" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Indicates that the related item[s] are following the specified government or industry rules regarding material, manufacturing process and or end product</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="RegulatoryComplianceQual" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code indicating the act or legislation</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="YesOrNoResponse" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Code indicating a Yes or No condition or response</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="RegulatoryComplianceID" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The ID relating to the RegulatoryComplianceQual, if applicable </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="RegulatoryAgency" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Used to indicate the organization responsible for validating or enforcing the RegulatoryComplianceQual when not otherwise specified by the RegulatoryComplianceQual</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Description" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Free-form textual description to clarify the related data elements and their content</xs:documentation>
                      <xs:appinfo>
                        <spsxs:example>New products only. Do not reuse packaging</spsxs:example>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Summary" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="TotalAmount" type="xs:decimal" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Total amount of the transaction. Sum of the item qty * price +/- the charges, allowances, and taxes, if sent</xs:documentation>
                <xs:appinfo>
                  <spsxs:example>4058.92</spsxs:example>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="TotalLineItemNumber" type="xs:int" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Sum of the total number of line items in this document</xs:documentation>
                <xs:appinfo>
                  <spsxs:example>3</spsxs:example>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
